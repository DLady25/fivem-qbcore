# Resources & grupos do QBCore
add_ace resource.qb-core command allow          # Allow qb-core to execute commands
add_ace qbcore.god command allow                # Allow all commands

# Inheritance
add_principal qbcore.god group.admin            # gods -> admin
add_principal qbcore.god qbcore.admin           # gods -> qbcore.admin
add_principal qbcore.admin qbcore.mod           # admin -> mod
